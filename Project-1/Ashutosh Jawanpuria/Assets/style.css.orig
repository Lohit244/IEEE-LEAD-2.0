#navbar {
    width: 100%;
    position: fixed;
    top: 0;
    overflow: hidden;
    margin: 0;
}

#navbar a {
    float: left;
    display: block;
    color: #f2f2f2;
    text-decoration: none;
    font-size: 17rem;
    text-align: center;
    text-decoration: none;
    padding: 15rem;
    margin-left: 8%;
    margin-right: 8%;
    opacity: 0.5;
}

#navbar a:hover {
    background-color: rgb(8, 63, 8);
    color: rgb(63, 6, 6);
}